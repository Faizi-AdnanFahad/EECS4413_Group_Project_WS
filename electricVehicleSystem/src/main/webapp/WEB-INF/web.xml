<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>Jersey REST Service</servlet-name>
		<servlet-class>org.glassfish.jersey.servlet.ServletContainer</servlet-class>
		<init-param>
			<param-name>jersey.config.server.provider.packages</param-name>
			<param-value>
				package
				business.controller.CatalogCntlr;business.controller.UserCntlr;business.controller.VehicleCntlr;business.controller.VehicleComparisionCntlr;business.controller.AdministratorCntlr;business.controller.ShoppingCartCntlr;</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>Jersey REST Service</servlet-name>
		<url-pattern>/rest/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>VehicleController</display-name>
		<servlet-name>VehicleController</servlet-name>
		<servlet-class>business.controller.VehicleCntlr.VehicleController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>VehicleController</servlet-name>
		<url-pattern>/VehicleController</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>VehicleComparisionCntlr</display-name>
		<servlet-name>VehicleComparisionCntlr</servlet-name>
		<servlet-class>
			business.controller.VehicleComparisionCntlr.VehicleComparisionCntlr
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>VehicleComparisionCntlr</servlet-name>
		<url-pattern>/VehicleComparisionCntlr</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>UserServlet</display-name>
		<servlet-name>UserServlet</servlet-name>
		<servlet-class>business.controller.UserCntlr.UserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/UserServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>RateController</display-name>
		<servlet-name>RateController</servlet-name>
		<servlet-class>
			business.controller.VehicleCntlr.RateController
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RateController</servlet-name>
		<url-pattern>/RateController</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>SignOutServlet</display-name>
		<servlet-name>SignOutServlet</servlet-name>
		<servlet-class>business.controller.UserCntlr.SignOutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SignOutServlet</servlet-name>
		<url-pattern>/SignOutServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>FeedbackController</display-name>
		<servlet-name>FeedbackController</servlet-name>
		<servlet-class>
			business.controller.AdministratorCntlr.FeedbackController
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FeedbackController</servlet-name>
		<url-pattern>/FeedbackController</url-pattern>
	</servlet-mapping>

</web-app>